<h1>Listing images</h1>

<table>
  <tr>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Name</th>
    <th>Capture time</th>
    <th>Model</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @images.each do |image| %>
  <tr>
    <td><%= image.latitude %></td>
    <td><%= image.longitude %></td>
    <td><%= image.name %></td>
    <td><%= image.capture_time %></td>
    <td><%= image.model %></td>
    <td><%= link_to 'Show', image %></td>
    <td><%= link_to 'Edit', edit_image_path(image) %></td>
    <td><%= link_to 'Destroy', image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Image', new_image_path %>
